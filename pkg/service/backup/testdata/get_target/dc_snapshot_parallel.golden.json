{
  "units": [
    {
      "keyspace": "system_auth",
      "tables": [
        "role_members",
        "role_permissions",
        "role_attributes",
        "roles"
      ],
      "all_tables": true
    },
    {
      "keyspace": "system_distributed",
      "tables": [
        "view_build_status",
        "service_levels",
        "cdc_streams_descriptions_v2",
        "cdc_generation_timestamps"
      ],
      "all_tables": true
    },
    {
      "keyspace": "system_distributed_everywhere",
      "tables": [
        "cdc_generation_descriptions_v2"
      ],
      "all_tables": true
    },
    {
      "keyspace": "system_traces",
      "tables": [
        "node_slow_log_time_idx",
        "sessions",
        "events",
        "sessions_time_idx",
        "node_slow_log"
      ],
      "all_tables": true
    },
    {
      "keyspace": "system_schema",
      "tables": [
        "types",
        "functions",
        "view_virtual_columns",
        "scylla_tables",
        "indexes",
        "triggers",
        "tables",
        "views",
        "computed_columns",
        "aggregates",
        "keyspaces",
        "columns",
        "dropped_columns"
      ],
      "all_tables": true
    }
  ],
  "dc": [
    "dc1",
    "dc2"
  ],
  "location": [
    "s3:backuptest-get-target"
  ],
  "retention": 3,
  "retention_days": 0,
  "rate_limit": [
    "100"
  ],
  "snapshot_parallel": [
    "10",
    "dc1:20"
  ],
  "continue": true,
  "purge_parallel": 1
}